    "@types/jasmine": "^2.6.0",
    "@types/jasminewd2": "^2.0.3",
    "@types/node": "^6.0.90",
    "codacy-coverage": "^2.0.3",
    "codelyzer": "^3.2.1",
    "connect-history-api-fallback": "^1.4.0",
    "http-proxy-middleware": "^0.17.4",
    "jasmine-core": "^2.8.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "~1.0.1",
