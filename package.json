{
  "name": "superfitschedule",
  "version": "2.4.0",
  "license": "UNLICENSED",
  "scripts": {
    "ng": "ng",
    "serve": "ng serve --host 0.0.0.0 --disable-host-check",
    "start": "npm run serve -- --proxy-config proxy.conf.json",
    "start:local": "npm run serve -- --proxy-config proxy.conf.local.json",
    "start:prod": "npm run start:local -- --prod -sm --watch=false --live-reload false",
    "start:sw": "npm run build && npm run start:liteserver",
    "start:liteserver": "lite-server -c liteserver_config.js",
    "build": "ng build --prod && node src/service-worker/rollup.js",
    "test": "ng test",
    "build-sm": "npm run build -- --vendor-chunk=false -sm --output-hashing=none",
    "explore-sm": "npm run build-sm && source-map-explorer dist/main.bundle.js",
    "test:once": "ng test --watch=false --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e --proxy-config proxy.conf.json",
    "ci": "npm run lint && npm run test:once && npm run e2e"
  },
  "private": true,
  "dependencies": {
    "@angular-redux/router": "^6.3.0",
    "@angular-redux/store": "^6.5.2",
    "@angular/animations": "^4.2.3",
    "@angular/common": "^4.2.3",
    "@angular/compiler": "^4.2.3",
    "@angular/compiler-cli": "^4.2.3",
    "@angular/core": "^4.2.3",
    "@angular/flex-layout": "2.0.0-beta.8",
    "@angular/forms": "^4.2.3",
    "@angular/http": "^4.2.3",
    "@angular/material": "^2.0.0-beta.7",
    "@angular/platform-browser": "^4.2.3",
    "@angular/platform-browser-dynamic": "^4.2.3",
    "@angular/platform-server": "^4.2.3",
    "@angular/router": "^4.2.3",
    "angular2-jwt": "^0.2.3",
    "core-js": "^2.4.1",
    "hammerjs": "^2.0.8",
    "lodash-es": "^4.17.4",
    "lodash.isequal": "^4.5.0",
    "moment-mini": "^2.18.1",
    "ng-recaptcha": "^2.1.1",
    "redux": "^3.7.0",
    "redux-observable": "^0.14.1",
    "rxjs": "^5.4.1",
    "typescript": "^2.4.0",
    "zone.js": "^0.8.12"
  },
  "devDependencies": {
    "@angular/cli": "^1.1.2",
    "@angular/service-worker": "^1.0.0-beta.15",
    "@types/hammerjs": "^2.0.34",
    "@types/jasmine": "2.5.38",
    "@types/node": "^6.0.78",
    "codacy-coverage": "^2.0.2",
    "codelyzer": "~2.0.0",
    "connect-history-api-fallback": "^1.3.0",
    "http-proxy-middleware": "^0.17.4",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.0",
    "rollup": "^0.42.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0"
  },
  "repository": {
    "type": "git",
